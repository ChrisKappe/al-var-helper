{
	"name": "al-var-helper",
	"description": "Al Var Helper Client",
	"author": "Rasmus Aaen",
	"license": "MIT",
	"version": "1.0.0",
	"publisher": "rasmus",
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples"
	},
	"engines": {
		"vscode": "^1.22.0"
	},
	"categories": [
		"Other"
	],
	"contributes": {
		"snippets": [
			{
				"language": "al",
				"path": "./snippets/al.json"
			}
		]
	},
	"activationEvents": [
		"onLanguage:al"
	],
	"main": "./out/src/extension",
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -p ./",
		"watch": "tsc -w -p ./",
		"update-vscode": "node ./node_modules/vscode/bin/install",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"dependencies": {
		"vscode": "^1.1.14",
		"vscode-languageclient": "^4.1.3"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.48",
		"@types/node": "^6.0.101",
		"typescript": "^2.7.2"
	}
}
